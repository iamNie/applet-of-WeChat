/**弹框*/

.drawer_box {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0px;
  background: rgba(0, 0, 0, 0.4);
  overflow: hidden;
}

.t_w {
  position: relative;
  width: 75%;
  height: 150px;
  text-align: center;
  top: 25%;
  background-color: #fff;
  left: 12.5%;
  border-radius: 10px;
}

.drawer_title {
  width: 100%;
  height: 50px;
  line-height: 50px;
  font-size: 13px;
  color: #000;
  font-weight: bold;
}

.drawer_content {
  height: 60px;
  line-height: 20px;
  width: 90%;
  margin: 0 auto;
}

.btn_ok {
  color: #2da66e;
  font-size: 14px;
  height: 40px;
  line-height: 40px;
}